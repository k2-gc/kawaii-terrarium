{
  "name": "kawaii-terrarium",
  "displayName": "Kawaii Terrarium",
  "description": "Cute Mofu characters living in your VS Code editor",
  "version": "1.0.0",
  "publisher": "k2-gc",
  "icon": "media/logo/icon.png",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/k2-gc/kawaii-terrarium"
  },
  "bugs": {
    "url": "https://github.com/k2-gc/kawaii-terrarium/issues"
  },
  "homepage": "https://github.com/k2-gc/kawaii-terrarium",
  "keywords": ["pet", "cute", "terrarium", "fun", "animation", "kawaii", "mofu"],
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kawaii-terrarium.summonMofus",
        "title": "Kawaii Terrarium: Summon Mofu on Secondary SideBar"
      },
      {
        "command": "kawaii-terrarium.summonFriend",
        "title": "Kawaii Terrarium: Summon Mofu on Panel"
      },
      {
        "command": "kawaii-terrarium.dismissFriend",
        "title": "Kawaii Terrarium: Dismiss Mofu from Panel"
      }
    ],
    "viewsContainers": {
      "secondarySidebar": [
        {
          "id": "kawaii-terrarium-secondary-sidebar",
          "title": "Kawaii Terrarium",
          "icon": "media/logo/icon.png"
        }
      ]
    },
    "views": {
      "kawaii-terrarium-secondary-sidebar": [
        {
          "id": "kawaii-terrarium-secondary-sidebar-view",
          "name": "Mofu Terrarium",
          "icon": "media/logo/icon.png",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:tsc": "tsc -watch -p ./",
    "watch:esbuild": "node esbuild.js --watch",
    "package": "tsc -p ./ && node esbuild.js --production",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.107.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  }
}
