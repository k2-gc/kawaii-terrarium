{
  "name": "kawaii-terrarium",
  "displayName": "kawaii-terrarium",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kawaii-terrarium.summonFriend",
        "title": "kawaii terrarium: Summon Aidoru"
      },
      {
        "command": "kawaii-terrarium.dismissFriend",
        "title": "kawaii terrarium: Dismiss Aidoru"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:tsc": "tsc -watch -p ./",
    "watch:esbuild": "node esbuild.js --watch",
    "package": "tsc -p ./ && node esbuild.js --production",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.107.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  }
}
